{
  "name": "svelte-flagpack",
  "version": "0.1.0",
  "description": "Gorgeous flag components for your Svelte project.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "src/Flag.svelte",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c && npm run generate-flags",
